{
  "name": "moc-oauth-client",
  "version": "1.0.3",
  "description": "OAuth client for Ministry of Commerce, Cambodia",
  "author": "Ministry of Commerce, Cambodia",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/SOMONSOUM/moc-oauth-client.git"
  },
  "homepage": "https://github.com/SOMONSOUM/moc-oauth-client",
